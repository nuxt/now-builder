  "devDependencies": {
    "@now/build-utils": "^0.10.1",
    "@nuxt/types": "^0.3.4",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.12.7",
    "@types/resolve-from": "^5.0.1",
    "@types/semver": "^6.2.0",
